#pragma once

void* operator new(size_t len);
void operator delete(void* p) noexcept;

inline void* operator new(size_t len, void* p) noexcept { return p; }
inline void* operator new[](size_t len, void* p) noexcept { return p; }
inline void operator delete(void*, void*) noexcept {}
inline void operator delete[](void*, void*) noexcept {}
